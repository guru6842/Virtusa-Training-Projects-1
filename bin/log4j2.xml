<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="Console" taget="SYSTEM_OUT">
			<PatternLayout pattern="%msg%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Logger name="process" level="info" additivity="false">
			<AppenderRef ref="Console" />
		</Logger>
		<Root level="info">
			<AppenderRef ref="Console" />
		</Root>
	</Loggers>
</Configuration>